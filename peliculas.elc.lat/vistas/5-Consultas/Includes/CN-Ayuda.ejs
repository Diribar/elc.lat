<!-- Ayuda -->
<div class="ayuda <%- locals.ayudaClass ? ayudaClass : 'absolute' %>">
	<i class="<%= iconos.faSolid %> <%= iconos.ayuda %> ayudaIconoForm clickIcono">
		<div class="clickVista ayudaMensForm ocultar">
			<% for (let ayuda of ayudas) { %>
				<% const {parrafo, dots} = ayuda %>
				<% if (parrafo) { %><p><%- parrafo %></p><% } %>
				<% if (dots) { %>
					<ul type="disc">
						<% for (let mensaje of dots) { %>
							<li><%- mensaje %></li>
						<% } %>
					</ul>
				<% } %>
			<% } %>
		</div>
	</i>
</div>
