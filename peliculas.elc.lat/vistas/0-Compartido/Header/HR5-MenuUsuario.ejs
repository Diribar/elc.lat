<% if (locals.usuario) { %>
	<div id="menuUsuario" class="clickVista menuForm menuFondo ocultar menuDerecha">
		<% for (let menu of menuUsuario) { %>
			<% if (menu.permInputs && (!locals.usuario || !usuario.rol.permInputs)) { continue } %>
			<a href="<%= menu.href %>" class="menuOpcion">
				<i class="<%= iconos.faSolid %> <%= menu.icono %> icono"></i>
				<span><%= menu.titulo %></span>
			</a>
			<hr>
		<% } %>
	</div>
<% } %>
